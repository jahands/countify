{
	"name": "dash",
	"private": true,
	"sideEffects": false,
	"scripts": {
		"build": "remix build",
		"dev:remix": "remix watch",
		"dev:wrangler": "cross-env NODE_ENV=development npm run wrangler",
		"dev": "npm-run-all build --parallel \"dev:*\"",
		"lint": "eslint \"app/**/*.[tj]s?(x)\" --cache --cache-strategy content --max-warnings=0",
		"start": "cross-env NODE_ENV=production npm run wrangler",
		"typecheck": "tsc",
		"wrangler": "wrangler pages dev ./public",
		"deploy:wrangler": "wrangler pages publish public --project-name countify-dash"
	},
	"dependencies": {
		"@headlessui/react": "^1.7.15",
		"@heroicons/react": "^2.0.18",
		"@remix-run/cloudflare": "^1.17.1",
		"@remix-run/cloudflare-pages": "^1.17.1",
		"@remix-run/css-bundle": "^1.17.1",
		"@remix-run/react": "^1.17.1",
		"cross-env": "^7.0.3",
		"isbot": "^3.6.8",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^3.19.0",
		"@remix-run/dev": "^1.17.1",
		"@remix-run/eslint-config": "^1.17.1",
		"@types/react": "^18.0.35",
		"@types/react-dom": "^18.0.11",
		"eslint": "^8.38.0",
		"eslint-config-custom": "*",
		"eslint-config-prettier": "^8.8.0",
		"npm-run-all": "^4.1.5",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"tailwindcss": "^3.3.2",
		"typescript": "^5.0.4",
		"wrangler": "^2.15.1"
	},
	"engines": {
		"node": ">=16.13.0"
	}
}
